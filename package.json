    "lodash": "^4.17.21",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-modules": "^4.1.3",
    "postcss-prefix-selector": "^1.10.0",
    "postcss-prefixer": "^2.1.3",
    "postcss-scss": "^4.0.0",
    "posthtml-css-modules": "^0.1.3",
    "posthtml-loader": "^2.0.1",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "ts-node": "~10.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  },
  "resolutions": {
    "webpack": "5.4.0"
  }

{
  "name": "netex",
  "version": "2.1.0-rc9",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --configuration=dev",
    "start:dev_server": "ng serve --configuration=dev_server",
    "start:prod": "ng serve --configuration=production",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && htmlhint",
    "e2e": "ng e2e",
    "build-prod": "ng build --configuration=production --output-hashing=none",
    "build-stable": "ng build --configuration=stable --output-hashing=none",
    "build-dev": "ng build --configuration=development --output-hashing=none",
    "build-dev-server": "ng build --configuration=development_server --output-hashing=none",
    "package-es5": "cd dist/netex && cat runtime-es5.js polyfills-es5.js main-es5.js > bundle-es5.js",
    "package-es2015": "cd dist/netex && cat runtime-es2015.js polyfills-es2015.js main-es2015.js > bundle-es2015.js",
    "bundle": "npm run build-prod netex && npm run package-es5 && npm run package-es2015",
    "prefix": "python3 prefixCSS.py --stage parse",
    "prefix:cleanup": "python3 prefixCSS.py --stage cleanup",
    "build:netex": "npm run prefix && npm run build-prod netex && node build-netex.js",
    "build:netex-stable": "npm run prefix && npm run build-stable netex && node build-netex.js",
    "build:netex-dev": "npm run prefix && npm run build-dev netex && node build-netex.js",
    "build:netex-dev-server": "npm run prefix && npm run build-dev-server netex && node build-netex.js",
    "build:drugstone": "npm run prefix && npm run build-prod netex && node build-netex.js && npm run prefix:cleanup",
    "build:drugstone-stable": "npm run prefix && npm run build-stable netex && node build-netex.js  && npm run prefix:cleanup",
    "build:drugstone-dev": "npm run prefix && npm run build-dev netex && node build-netex.js && npm run prefix:cleanup",
    "build:drugstone-dev-server": "npm run prefix && npm run build-dev-server netex && node build-netex.js && npm run prefix:cleanup"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/elements": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/localize": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@ng-bootstrap/ng-bootstrap": "^13.1.1",
    "@ng-select/ng-select": "^9.1.0",
    "animate.css": "^4.1.1",
    "bulma": "^0.9.2",
    "bulma-toast": "^2.4.1",
    "bulma-tooltip": "^3.0.2",
    "convert-svg-to-png": "^0.6.4",
    "document-register-element": "^1.7.2",
    "dom-to-image-cross-origin": "^2.6.7",
    "json5": "^2.2.0",
    "lodash.merge": "^4.6.2",
    "ng-multiselect-dropdown": "^0.3.8",
    "primeng": "^14.2.3",
    "rxjs": "^6.5.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.1.0",
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@popperjs/core": "^2.11.8",
    "@types/jasmine": "~3.7.7",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^15.12.2",
    "codelyzer": "~6.0.2",
    "concat": "^1.0.3",
    "fs-extra": "^10.0.0",
    "htmlhint": "^1.1.4",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lodash": "^4.17.21",
    "raw-loader": "^4.0.2",
    "ts-node": "~10.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4",
    "webpack": "5.76.3"
  },
  "overrides": {
    "glob-parent": "5.1.2",
    "node-forge": "1.3.0",
    "minimatch": "~3.0.5",
    "semver": ">=7.5.2",
    "xml2js": ">=0.5.0",
    "tough-cookie": ">=4.1.3",
    "codelyzer": {
      "@angular/core": ">=10.2.5"
    },
    "ws": ">=8.17.1"
  }
}
